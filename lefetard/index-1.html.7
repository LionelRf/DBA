<section class="clearfix box-body" itemprop="articleBody">
<p><span style="font-family: 'courier new', courier, monospace;">SELECT grantee AS user, CONCAT(table_schema, '.', table_name) AS table,&nbsp;</span><br /><span style="font-family: 'courier new', courier, monospace;">&nbsp; &nbsp; CASE&nbsp;</span><br /><span style="font-family: 'courier new', courier, monospace;">&nbsp; &nbsp; &nbsp; &nbsp; WHEN COUNT(privilege_type) = 7 THEN 'ALL'</span><br /><span style="font-family: 'courier new', courier, monospace;">&nbsp; &nbsp; &nbsp; &nbsp; ELSE ARRAY_TO_STRING(ARRAY_AGG(privilege_type), ', ')</span><br /><span style="font-family: 'courier new', courier, monospace;">&nbsp; &nbsp; END AS grants</span><br /><span style="font-family: 'courier new', courier, monospace;">FROM information_schema.role_table_grants</span><br /><span style="font-family: 'courier new', courier, monospace;">GROUP BY table_name, table_schema, grantee;</span></p>
