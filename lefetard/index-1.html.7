<section class="clearfix box-body" itemprop="articleBody">
<p><pre>yum install oracle-instantclient12.2-basic-12.2.0.1.0-1.x86_64.rpm<br />yum install oracle-instantclient12.2-devel-12.2.0.1.0-1.x86_64.rpm<br />yum install oracle-instantclient12.2-jdbc-12.2.0.1.0-1.x86_64.rpm<br />yum install oracle-instantclient12.2-sqlplus-12.2.0.1.0-1.x86_64.rpm<br />yum -y install perl-DBI<br />yum install gcc<br />yum install glibc-devel<br />yum install perl-ExtUtils-MakeMaker -y<br />yum install perl-open.noarch -y</pre>
<pre><br /><br />export ORACLE_HOME=/usr/lib/oracle/12.2/client64<br />export LD_LIBRARY_PATH=/usr/lib/oracle/12.2/client64/lib<br /><br />tar -xvzf ora2pg-23.0.tar.gz<br />cd ora2pg-23.0/<br />perl Makefile.PL<br />make &amp;&amp; make install<br /><br />tar -xvzf DBD-Oracle-1.83.tar.gz<br />cd DBD-Oracle-1.83<br />perl Makefile.PL<br />make &amp;&amp; make install</pre></p>
